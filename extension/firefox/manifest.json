{
  "manifest_version": 2,
  "name": "DEEDS++ [Firefox edition]",
  "version": "2.3.2",
  "description": "Adds dark mode and rounded corners to DEEDS.",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "logo.png"
  },
  "permissions": ["activeTab", "storage"],
  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["content.js"],
    "matches": ["*://deeds.cgs.vic.edu.au/*"]
  }]

}
