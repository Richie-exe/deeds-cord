{
  "manifest_version": 3,
  "name": "DEEDS++ [Chrome edition]",
  "version": "2.3.2",
  "description": "Adds dark mode and rounded corners to DEEDS.",
  "icons": {
      "128": "logo.png",
      "16": "logo.png",
      "48": "logo.png"
   },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "storage"],
  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["content.js"],
    "matches": ["*://deeds.cgs.vic.edu.au/*"]
  }]

}
